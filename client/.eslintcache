der} history={history} />\r\n      <ConfirmAddressModal classes={classes} addressToConfirm={addressFromServerToBeConfirmed} open={confirmAddressModalOpen} handleClose={() => setConfirmAddressModalOpen(false)} handleConfirmAddress={handleConfirmAddress} handleRejectAddress={handleRejectAddress} />\r\n    </div>\r\n  );\r\n};\r\nexport default Checkout;\r\n","C:\\projects\\ellen-silverman-new\\client\\src\\store\\actions\\story-actions.js",[],"C:\\projects\\ellen-silverman-new\\client\\src\\store\\actions\\nav-actions.js",[],"C:\\projects\\ellen-silverman-new\\client\\src\\components\\artworks\\SeriesList.js",[],"C:\\projects\\ellen-silverman-new\\client\\src\\components\\artworks\\SeriesPage.js",[],"C:\\projects\\ellen-silverman-new\\client\\src\\components\\layout\\UserMenu.js",["448"],"import React, { useState, useEffect } from 'react'\r\nimport { Link } from \"react-router-dom\";\r\nimport { useDispatch, useSelector } from 'react-redux'\r\nimport firebase from \"firebase\";\r\nimport { ReactComponent as UserIcon } from \"../../icons/userIcon.svg\";\r\nimport { Badge, Icon, ClickAwayListener } from '@material-ui/core'\r\nimport { makeStyles } from '@material-ui/styles'\r\nimport ShoppingCartIcon from '@material-ui/icons/ShoppingCart';\r\nimport AccountCircleIcon from '@material-ui/icons/AccountCircle';\r\n\r\nconst useStyles = makeStyles({\r\n  icon: {\r\n    height: 40,\r\n    textAlign: \"center\",\r\n    width: \"100%\",\r\n    display: \"block\",\r\n    '& svg': {\r\n      color: \"black\",\r\n      fontSize: 40,\r\n      '&:hover': {\r\n        color: \"blue\"\r\n      }\r\n    }\r\n  },\r\n  badge: {\r\n    right: -20,\r\n    top: -24,\r\n    pointerEvents: \"none\",\r\n    // border: `2px solid black`,\r\n    padding: '0 4px',\r\n  },\r\n})\r\n\r\nconst UserMenu = ({ hideMenu }) => {\r\n  const dispatch = useDispatch()\r\n  const user = useSelector((state) => state.user)\r\n  const cart = useSelector((state) => state.cart)\r\n  const [showMenu, setShowMenu] 